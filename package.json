{
  "name": "custom-hooks",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "lint": "npm run prettier-styles && npm run stylelint",
    "stylelint": "stylelint ./src/assets/**/*.{scss,css}",
    "prettier-styles": "prettier --write ./src/assets/**/*.{scss,css}",
    "prettier-scripts": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "pre-commit": "npx lint-staged",
    "server1": "ts-node ./src/server.ts",
    "server": "tsc ./src/server.ts && node ./src/server.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,scss,css}": "prettier --write",
    "src/assets/**/*.{scss,css}": "stylelint"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^1.0.6",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "aws-amplify": "^3.3.26",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "node-sass": "^4.14.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/node-logger": "^6.1.21",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.1.21",
    "@types/react-router-dom": "^5.1.7",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "ts-node": "^9.1.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:4000"
}
